						<script>
							$(document).ready(function(){
                                $(".css_switcher").click(function(e){
									e.preventDefault();
									if($.cookie('style') != $(this).data("style"))
									{
										$.cookie('style', $(this).data("style"));
										$('.css_switcher').css("font-weight","normal");
										$(this).css("font-weight","700");
										$('#css').attr('href', $(this).data("style"));
										//location.reload();
									}
								});
								
                                $(".css_switcher").each(function(i){
									if($.cookie('style') == $(this).data("style"))
									{
										$(this).css("font-weight","700");
										$('#css').attr('href', $(this).data("style"));
									}
								});
							});	
